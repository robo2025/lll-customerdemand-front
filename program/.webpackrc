{
  "entry": "src/index.js",
  "extraBabelPlugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    },
     "production": {
      "extraBabelPlugins": [
        "transform-remove-console"
      ]
    }
  },
  "ignoreMomentLocale": true,
  "disableCSSModules": true,
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
